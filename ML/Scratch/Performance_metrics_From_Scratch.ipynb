   "source": [
    "def accuracy_metrics(actual, predicted):\n",
    "    correct = 0.0\n",
    "    for i in range(len(actual)):\n",
    "        if actual[i] == predicted[i]:\n",
    "            correct += 1.0\n",
    "    return (correct / len(actual)) * 100"
   ]
